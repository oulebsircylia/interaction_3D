# d√©finition des variables utilise pour la compilation
CXX= g++
SOURCE=fraclal_jul.cpp
EXEC= julia
SUP=rm -rf
CXXFLAGS=-lopencv_core -lopencv_highgui -lopencv_imgproc -std=c++11 -lpthread
all:	

	$(CXX) -Wall $(SOURCE) -o $(EXEC) $(CXXFLAGS)
	./julia

mrproper:clean
	$(SUP) *.o
clean:
	 $(SUP) *.o


